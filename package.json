{
  "name": "interactive-probation-map",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www --config ./package.json",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "govuk-frontend": "^2.9.0",
    "helmet": "^3.16.0",
    "http-errors": "~1.6.2",
    "jest": "^24.7.1",
    "morgan": "^1.9.1",
    "node-sass-middleware": "0.11.0",
    "nodemon": "^1.18.10",
    "nunjucks": "^3.2.0",
    "ol": "^6.1.1"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "watch": [
      "views",
      "routes",
      "public"
    ]
  },
  "ext": "js,json,html,css,scss,sass"
